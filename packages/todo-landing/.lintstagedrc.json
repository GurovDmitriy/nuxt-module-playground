{
  "**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --fix --ignore-path .eslintignore",
  "**/*.{vue,css,less,scss,html}": "stylelint --fix --allow-empty-input --ignore-path .stylelintignore",
  "**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,scss,less,md,json}": [
    "prettier --write --ignore-path .prettierignore"
  ]
}
